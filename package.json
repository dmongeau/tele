{
  "name": "tele",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "export_config": "eval $(cat .config | sed 's/^/export /')",
    "build:guide": "npm run export_config && ./vendor/zap2xml.pl -z -e -u $TVGUIDE_USERNAME -p $TVGUIDE_PASSWORD -o ./resources/guide.xml",
    "build:playlist": "node ./index.js",
    "sync:bundles": "npm run export_config && rsync -avh -e ssh ./bundles/ \"${PLEX_PLUGINS_PATH// /\\ }/\"",
    "sync:resources": "npm run export_config && rsync -avh -e ssh ./resources/ \"${PLEX_PLUGINS_PATH// /\\ }/IPTV.bundle/Contents/Resources/\"",

    "build": "npm run build:playlist && npm run build:guide",
    "sync": "npm run sync:bundles && npm run sync:resources",
    "start": "npm run build && npm run sync"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^2.0.0",
    "jsdom": "^9.11.0",
    "lodash": "^4.17.4",
    "request": "^2.79.0"
  }
}
